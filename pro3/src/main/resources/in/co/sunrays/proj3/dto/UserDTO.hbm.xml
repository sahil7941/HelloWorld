<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="in.co.sunrays.proj3.dto.UserDTO" table="USERDTO">
        <id name="id" type="long" access="field">
            <column name="ID" />
            <generator class="increment" />
        </id>
        
        
        
        <property name="fname" type="java.lang.String">
            <column name="FNAME" />
        </property>
        
        <property name="lname" type="java.lang.String">
            <column name="LNAME" />
        </property>
        
        <property name="login" type="java.lang.String">
            <column name="LOGIN" />
        </property>
        
        <property name="password" type="java.lang.String">
            <column name="PASSWORD" />
        </property>
        
        <!-- <property name="confirmPassword" type="java.lang.String">
            <column name="CONFIRMPASSWORD" />
        </property> -->
        
        <property name="dob" type="java.util.Date">
            <column name="DOB" />
        </property>
        
        <property name="mobileNo" type="java.lang.String">
            <column name="MOBILENO" />
        </property>
        
        <property name="roleId" type="long">
            <column name="ROLEID" />
        </property>
        
        <property name="unSuccessfulLogin" type="int">
            <column name="UNSUCCESSFULLOGIN" />
        </property>
        
        <property name="gender" type="java.lang.String">
            <column name="GENDER" />
        </property>
        
        <property name="lastLogin" type="java.sql.Timestamp">
            <column name="LASTLOGIN" />
        </property>
        
        <property name="locks" type="java.lang.String" access="field">
            <column name="LOCKS" />
        </property>
        
        <property name="registredIP" type="java.lang.String">
            <column name="REGISTREDIP" />
        </property>
        
        <property name="lastLoginIP" type="java.lang.String">
            <column name="LASTLOGINIP" />
        </property>
        <property name="createdBy" type="java.lang.String" access="field">
            <column name="CREATEDBY" />
        </property>
        <property name="modifiedBy" type="java.lang.String" access="field">
            <column name="MODIFIEDBY" />
        </property>
        
        <property name="createdDateTime" type="java.sql.Timestamp" access="field">
            <column name="CREATEDDATETIME" />
        </property>
        
        <property name="modifiedDateTime" type="java.sql.Timestamp" access="field">
            <column name="MODIFIEDDATETIME" />
        </property>
    </class>
</hibernate-mapping>
